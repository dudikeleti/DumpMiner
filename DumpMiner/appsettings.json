{
  "Application": {
    "General": {
      "SymbolCachePath": "c:\\dev",
      "DefaultTimeoutMs": 60000,
      "AutoCheckUpdates": true,
      "MaxRecentFiles": 10
    },
    "Appearance": {
      "Theme": "Dark",
      "AccentColor": "#FF647687",
      "FontSize": "Normal",
      "ShowDetailedTooltips": true,
      "EnableAnimations": true
    },
    "AI": {
      "DefaultProvider": "OpenAI",
      "MaxTokens": 4000,
      "TimeoutSeconds": 60,
      "EnableCaching": true,
      "CacheExpirationMinutes": 60,
      "MaxConversationHistory": 20,
      "MaxAutoFunctionCalls": 5,
      "MaxObjectAnalysisDepth": 3,
      "StackAnalysis": {
        "MaxDetailedThreads": 10,
        "MaxFramesPerThread": 30,
        "MaxTotalPayloadChars": 40000,
        "FilterSystemCode": true
      },
      "Providers": {
        "OpenAI": {
          "ApiKey": "",
          "Model": "gpt-4",
          "BaseUrl": "https://api.openai.com/v1",
          "Temperature": 0.7,
          "IsEnabled": true,
          "MaxTokens": 4000,
          "TimeoutSeconds": 60
        },
        "Anthropic": {
          "ApiKey": "",
          "Model": "claude-3-sonnet-20240229",
          "BaseUrl": "https://api.anthropic.com",
          "Temperature": 0.7,
          "IsEnabled": true
        },
        "Google": {
          "ApiKey": "",
          "Model": "gemini-pro",
          "BaseUrl": "https://generativelanguage.googleapis.com",
          "Temperature": 0.7,
          "IsEnabled": false
        }
      }
    },
    "Advanced": {
      "EnableDebugLogging": false,
      "LogLevel": "Information",
      "EnableProfiling": false,
      "MaxLogFileSizeMB": 10,
      "MemoryWarningThresholdMB": 1024
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Debug",
      "Serilog.Sinks.Datadog.Logs"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Warning",
        "System": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "DumpMiner.Services.AI": "Debug",
        "DumpMiner.Operations": "Information",
        "DumpMiner.Debugger": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/dumpminer-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "buffered": true,
          "flushToDiskInterval": "00:00:01",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/dumpminer-readable-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Debug",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss.fff} [{Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithEnvironmentUserName"
    ],
    "Properties": {
      "Application": "DumpMiner",
      "Environment": "Development"
    }
  },
  "DatadogLogging": {
    "Enabled": false,
    "ApiKey": "",
    "Source": "csharp",
    "Service": "dumpminer",
    "Host": "",
    "Tags": [
      "env:development",
      "version:2.0.0"
    ]
  }
} 